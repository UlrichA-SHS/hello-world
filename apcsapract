public static boolean isSelfDivisor(int number){ 

String test = number; 

for(int i = 0; i < test.length(); i++){ 

if((test.charAt(i) % test.charAt(i) != 0) ||  (test.charAt(i) != 0)){ 

return false; 

} else { 

return true; 

} 

} 



public static int [] firstNameSelfDivisors(int start, int num){ 

int arr [] = new int [num] 

for(int i = start; i<= num; i++) {

int k = 0; 

arr[k] += i; 

k++; 

}

return arr; 

} 
